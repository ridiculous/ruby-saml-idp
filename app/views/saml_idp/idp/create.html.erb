<!DOCTYPE html>
<html>
<head>
<meta charset="utf-8">
<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
</head>
<body>
<%= form_tag(@saml_acs_url) do %>
  <%= hidden_field_tag("SAMLResponse", @saml_response) %>
  <%= submit_tag "Submit" %>
<% end %>
<script type="text/javascript">
  var token_container = document.getElementsByTagName('div')[0]
  token_container.parentElement.removeChild(token_container)
</script>
</body>
</html>
